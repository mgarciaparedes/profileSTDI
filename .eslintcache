rrent}\r\n                                show={show}\r\n                                placement=\"top\"\r\n                              >\r\n                                {(props) => (\r\n                                  <Tooltip id=\"overlay-example\" {...props}>\r\n                                    Profile copied to clipboard!\r\n                                  </Tooltip>\r\n                                )}\r\n                              </Overlay> */}\r\n                              <Button\r\n                                //ref={target}\r\n                                onClick={() => {\r\n                                  setShow(!show);\r\n                                  copyToClipboard(username);\r\n                                  Swal.fire(\r\n                                    \"Text copied to clipboard!\",\r\n                                    \"\",\r\n                                    \"success\"\r\n                                  );\r\n                                }}\r\n                              >\r\n                                <span>\r\n                                  <i className=\"bi bi-clipboard\" />\r\n                                </span>\r\n                                &nbsp; Copy Link\r\n                              </Button>\r\n                            </div>\r\n\r\n                            {/*Final Botón Copy Link */}\r\n\r\n                            {/*Inicio Botón Share Link */}\r\n                            <Button\r\n                              variant=\"success\"\r\n                              onClick={() => {\r\n                                shareLink(username);\r\n                              }}\r\n                              className=\"mt-3\"\r\n                            >\r\n                              <span>\r\n                                <i className=\"bi bi-share\" />\r\n                              </span>\r\n                              &nbsp; Share Link\